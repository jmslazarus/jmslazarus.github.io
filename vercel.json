{
    "version": 2,
    "name": "jmslazarus.github.io",
    "builds": [
        {
            "src": "./hack-server.js",
            "use": "@vercel/node"
        },
        {
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/terminal/(.*)",
            "dest": "/hack-server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
 
    ]
}